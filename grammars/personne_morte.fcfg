%start S

S[SEM=<EstMort(?vp)>] -> VP[SEM=?vp]
VP[SEM=?n] -> N[SEM=?n] V AD
N[SEM=?np] -> Nprop[SEM=?np]
V -> 'est'
AD -> 'mort' | 'morte'

Nprop[SEM=<Pikachu>] -> 'Pikachu'
Nprop[SEM=<Évioli>] -> 'Évioli'
Nprop[SEM=<Dracaufeu>] -> 'Dracaufeu'
Nprop[SEM=<Ectoplasma>] -> 'Ectoplasma'
Nprop[SEM=<Carapuce>] -> 'Carapuce'
Nprop[SEM=<Herbizzare>] -> 'Herbizzare'